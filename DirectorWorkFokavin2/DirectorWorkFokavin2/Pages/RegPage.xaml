<Page x:Class="DirectorWorkFokavin2.Pages.RegPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DirectorWorkFokavin2.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegPage">

    <Grid>
        <StackPanel>
            <TextBlock Text="Name"/>
            <TextBox x:Name="BName"/>
            <TextBlock Text="FirstName"/>
            <TextBox x:Name="BFirstName"/>
            <TextBlock Text="SecondName"/>
            <TextBox x:Name="BSecondName"/>
            <TextBlock Text="Login"/>
            <TextBox x:Name="BLogin"/>
            <TextBlock Text="Password"/>
            <TextBox x:Name="BPassword"/>
            <Button x:Name="TBRegistracia" Content="Войти" Click="TBRegistracia_Click"/>
        </StackPanel>
    </Grid>
</Page>
