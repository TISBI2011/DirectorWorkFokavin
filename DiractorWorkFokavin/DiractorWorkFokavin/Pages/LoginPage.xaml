<Page x:Class="DiractorWorkFokavin.Pages.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DiractorWorkFokavin.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginPage">

    <Grid>
        <StackPanel>
            <TextBlock Text="Login"/>
            <TextBox x:Name="BLogin"/>
            <TextBlock Text="Password"/>
            <TextBox x:Name="BPassword"/>
            <TextBlock Text="Name"/>
            <TextBox x:Name="BName"/>
            <TextBlock Text="SureName"/>
            <TextBox x:Name="SureName"/>
            <TextBlock Text="NickName"/>
            <TextBox x:Name="BNickName"/>
            <Button x:Name="Login" Content="Login" Click="Login_Click"/>
        </StackPanel>
        
    </Grid>
</Page>
